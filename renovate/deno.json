{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "jsonata",
      "fileFormat": "json",
      "managerFilePatterns": ["/deno.jsonc?/"],
      "matchStrings": [
        "imports.*[$ ~> /npm:/ ][].$substring($, 4).[$split($, \"@\")].{\"depName\": $[0], \"currentValue\": $[1]}"
      ],
      "datasourceTemplate": "npm"
    }
  ]
}
